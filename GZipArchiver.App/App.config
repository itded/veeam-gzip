<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!-- the number of working threads. Set 0 to use all logical processors -->
    <add key="workThreadCount" value="0"/>
    <!-- the size of a file chunk to be processed -->
    <add key="blockSize" value="1048576"/>
    <!-- the capacity of the input file chunk queue -->
    <add key="queueInSize" value="100"/>
    <!-- the capacity of the output file chunk queue -->
    <add key="queueOutSize" value="100"/>
  </appSettings>
</configuration>